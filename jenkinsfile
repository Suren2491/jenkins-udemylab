pipeline {
    agent any
    tools { go 'go-1.23.6' } // Comes from the jenkins global config

     environment {
        ENV = "${env.BRANCH_NAME == 'master' ? 'PROD' : 'DEV'}"

        stages {
        stage('Build') {
            steps {
                sh 'build.sh' // Run the build.sh asset
            }
        }
        stage('Test') {
            steps {
                sh 'test.sh' // Run the test.sh asset
            }
        }
    }
}

}
    
